<header class="navbar navbar-fixed-top
navbar-inverse">
  <div class="container">
    <%= link_to "sample app", "#", id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to t(".english"), locale: :en %></li>
        <li><%= link_to t(".vietnamese"), locale: :vi %></li>
        <li><%= link_to t(".home_nav"), static_pages_home_path %></li>
        <li><%= link_to t(".help_nav"), static_pages_help_path %></li>
        <% if logged_in? %>
          <li><%= link_to t(".profile"), current_user %></li>
          <li><%= link_to t(".setting"), "#" %></li>
          <li class="divider"></li>
          <li> <%= link_to t(".log_out"), logout_path, method: :delete %></li>
        <% else %>
          <li><%= link_to t(".log_in"), login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
