<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">

    <nav>
      <ul class="nav navbar-nav navbar-left">
        <li><%= link_to "sample app", "#", id: "logo" %></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "English", locale:"en" %></li>
        <li><%= link_to "Vietnamese", locale:"vi" %></li>
        <li><%= link_to (t(".home_nav")), static_page_home_path %></li>
        <li><%= link_to (t(".help_nav")), static_page_help_url %></li>

        <% if logged_in? %>
          <li><%= link_to t(".profile"), current_user %></li>
          <li><%= link_to t(".setting"), '#' %></li>
          <li class="divider"></li>
          <li> <%= link_to t(".log_out"), logout_path, method: :delete %></li>
        <% else %>
          <li><%= link_to t(".log_in"), login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>